<h1>Wobs</h1>

<ul>
  <% @wobs.each do |wob| %>
    <li>
      <%= link_to wob, wob %> (<%= wob.user %>)
      <% if wob.user == current_user %>
        <%= link_to 'Edit', edit_wob_path(wob) %>
        <%= link_to 'Destroy', wob, method: :delete, data: { confirm: 'Are you sure?' } %>
      <% end %>
    </li>
  <% end %>
</ul>

<%= link_to 'New Wob', new_wob_path %>
